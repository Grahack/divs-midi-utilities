<?xml version="1.0"?>

<!--
Conventional piano keyboard, like most tracker software.
-->

<qwertymidi>

<map key="escape" action="exit" />
<map key="f1" action="stay-down-octave" />
<map key="f2" action="stay-up-octave" />
<map key="f3" action="stay-down-note" />
<map key="f4" action="stay-up-note" />
<map key="shift" action="shift-up-octave" />
<map key="control" action="shift-down-octave" />
<map key="space" action="panic" />

<!-- Upper octave -->

<map key="tab" action="F3" />
<map key="1" action="F#3" />
<map key="Q" action="G3" />
<map key="2" action="G#3" />
<map key="W" action="A3" />
<map key="3" action="A#3" />
<map key="E" action="B3" />
<map key="R" action="C4" />
<map key="5" action="C#4" />
<map key="T" action="D4" />
<map key="6" action="D#4" />
<map key="Y" action="E4" />
<map key="U" action="F4" />
<map key="8" action="F#4" />
<map key="I" action="G4" />
<map key="9" action="G#4" />
<map key="O" action="A4" />
<map key="0" action="A#4" />
<map key="P" action="B4" />
<map key="open-bracket" action="C5" />
<map key="equals" action="C#5" />
<map key="close-bracket" action="D5" />
<map key="backspace" action="D#5" />
<map key="backslash" action="E5" />

<!-- Lower octave -->

<map key="Z" action="A2" />
<map key="S" action="A#2" />
<map key="X" action="B2" />
<map key="C" action="C3" />
<map key="F" action="C#3" />
<map key="V" action="D3" />
<map key="G" action="D#3" />
<map key="B" action="E3" />
<map key="N" action="F3" />
<map key="J" action="F#3" />
<map key="M" action="G3" />
<map key="K" action="G#3" />
<map key="comma" action="A3" />
<map key="L" action="A#3" />
<map key="period" action="B3" />
<map key="slash" action="C4" />
<map key="single-quote" action="C#4" />
<map key="enter" action="D#4" />

</qwertymidi>
